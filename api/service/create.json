{
    "name": "创建服务",
    "method": "POST",
    "params": {
        "code": {
            "rem": "服务号",
            "need": true,
            "reg": "^\\w{3,16}$"
        },
        "name": {
            "rem": "服务名",
            "len": [1, 32]
        },
        "brief": {
            "rem": "介绍"
        },
        "logo": {
            "rem": "图片"
        },
        "token": {
            "rem": "密钥"
        }
    },
    "grant": {
        "$U": "需要登录"
    },
    "error": {
        "405": "服务号被占用"
    },
    "ret": {
        "no": 200,
        "msg": ""
    }
}